<h1>Listing twitters</h1>

<table>
  <tr>
    <th>User</th>
    <th>Content</th>
    <th>Created at</th>
    <th></th>
    <th></th>
    <th></th>
  </tr>

<% @twitters.each do |twitter| %>
  <tr>
    <td><%= twitter.user_id %></td>
    <td><%= twitter.content %></td>
    <td><%= twitter.created_at %></td>
  </tr>
<% end %>
</table>

